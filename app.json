{
  "expo": {
    "name": "fastRide",
    "slug": "fastride",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Icon.png",
    "scheme": "fastride",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./ios/fastRide/GoogleService-Info.plist",
      "bundleIdentifier": "com.nishexpo2.fastRide",
      "config": {
        "googleMapsApiKey": "AIzaSyCLVlmceJF7PXpWwsmc9OUKDLTNlAGVXms"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show addresses on the map."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nishexpo2.fastRide",
      "googleServicesFile": "android/app/google-services.json",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCLVlmceJF7PXpWwsmc9OUKDLTNlAGVXms"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/auth",
      "@react-native-firebase/app",
      "@react-native-google-signin/google-signin",
      "@react-native-firebase/crashlytics",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Icon.png",
          "resizeMode": "cover",
          "backgroundColor": "#000000"
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Show current location on map"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "77a156ab-266f-471f-8b09-686de7ed2128"
      }
    },
    "owner": "nishexpo2"
  }
}
